.main{
  width: 700rpx;
  margin: auto;
}
swiper{
  width: 700rpx;
  height: 280rpx;
  margin: auto;
}
swiper-item{
  width: 100%;
  height:280rpx;
}
swiper image{
  width: 100%;
  height: 100% !important;
}
.float_left{
  float: left;
}
.float_right{
  float: right;
}
.img_box{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
}
.img_box image{
  width: 100%;
  height:100%;
  vertical-align: bottom;
}
.user_info{
  border-bottom: 1rpx solid #ccc;
  padding: 20rpx 20rpx;
}
.clear::after{
  content: "";
  display: block;
  clear: both;
}
.list{
  width: 33%;
  height: 188rpx;
  float: left;
  text-align: center;
}
.list image{
  width: 64rpx;
  height: 64rpx;
  display: block;
  margin:15rpx auto;
}
